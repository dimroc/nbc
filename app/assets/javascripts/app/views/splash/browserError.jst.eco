<div class="container">
  <p class="span12 height100"/>

  <h1>New Block City</h1>
  <table>
    <tbody>

      <tr>
        <% for world in @worlds: %>
          <td class="<%= _(world.name).underscored() %> icon centered">
            <img src="<%= world.icon_path() %>" alt="<%= world.name %>">
          </td>
        <% end %>
      </tr>
    </tbody>
  </table>
  <h2>Error!</h2>
  <p>This application requires <a href="http://www.google.com/chrome">Chrome!</a> version 23+.</p>
</div>
