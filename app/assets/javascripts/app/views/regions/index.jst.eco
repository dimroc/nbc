<h1>Listing Regions</h1>
<div class="well">
  <ul class="nav nav-list">
    <% for region in @regions: %>
      <li class="row-fluid" data-id="<%= region.id %>">
        <a class="span2" data-type="show"><%= region.name %></a>
        <a class="span2" data-type="edit">Edit</a>
        <a class="span2" data-type="destroy">Destroy</a>
      </li>
    <% end %>
  </ul>
</div>

<p><a data-type="new">New Region</a></p>
