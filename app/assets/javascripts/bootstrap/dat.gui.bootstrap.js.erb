$(function() {
  var navigateToRegions = function() {
    App.instance.navigate('/regions')
  };

  App.World.one('allLoaded', function() {
    var gui = new dat.GUI();

    var options = {
      "See Regions": navigateToRegions
    };

    gui.add(options, "See Regions");
    gui.closed = true

    <% if Rails.env.production? %>
        <%= gui.domElement.hidden = true %>
    <% end %>
  });
});
